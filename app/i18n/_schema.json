{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "covid-tracker i18n JSON Schema",
    "required": [
        "data-label-active",
        "data-label-recoveries",
        "data-label-deaths",
        "data-label-last-update",
        "data-source",
        "source-code",
        "footer-icons-attr-first",
        "footer-icons-attr-second",
        "country-selection-entire-world"
    ],
    "properties": {
        "data-label-active": {
            "$id": "#/properties/data-label-active",
            "type": "string",
            "default": "",
            "examples": [
                "Active Cases :"
            ]
        },
        "data-label-recoveries": {
            "$id": "#/properties/data-label-recoveries",
            "type": "string",
            "default": "",
            "examples": [
                "Recoveries :"
            ]
        },
        "data-label-deaths": {
            "$id": "#/properties/data-label-deaths",
            "type": "string",
            "default": "",
            "examples": [
                "Deaths :"
            ]
        },
        "data-label-last-update": {
            "$id": "#/properties/data-label-last-update",
            "type": "string",
            "default": "",
            "examples": [
                "Last Update :"
            ]
        },
        "data-source": {
            "$id": "#/properties/data-source",
            "type": "string",
            "default": "",
            "examples": [
                "Data source :"
            ]
        },
        "source-code": {
            "$id": "#/properties/source-code",
            "type": "string",
            "default": "",
            "examples": [
                "Source Code :"
            ]
        },
        "footer-icons-attr-first" : {
            "$id": "#/properties/source-code",
            "type": "string",
            "default": "",
            "examples": [
                "Icons made by"
            ]
        },
        "footer-icons-attr-second" : {
            "$id": "#/properties/source-code",
            "type": "string",
            "default": "",
            "examples": [
                "from"
            ]
        },
        "country-selection-entire-world" : {
            "$id": "#/properties/source-code",
            "type": "string",
            "default": "",
            "examples": [
                "from"
            ]
        }
    }
}
